{
  "name": "electron-print-server",
  "description": "Print server for printing URLs on local printers",
  "author": "jbot <monstion@gmail.com>",
  "homepage": "https://github.com/vast-ru/electron-print-server",
  "version": "0.6.0",
  "license": "MIT",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "html-loader": "1.0.0-alpha.0",
    "jspdf": "^2.5.1",
    "pdf-lib": "^1.17.1",
    "source-map-support": "0.5.12"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron/remote": "^2.0.1",
    "@types/express": "4.17.0",
    "axios": "~0.21.2",
    "babel-preset-flow": "^6.23.0",
    "body-parser": "1.19.0",
    "content-type": "~1.0.4",
    "debug": "4.1.1",
    "electron": "15.3.5",
    "electron-settings": "^4.0.0",
    "electron-webpack": "2.7.2",
    "electron-webpack-vue": "2.3.0",
    "express": "4.17.1",
    "flow-bin": "^0.86.0",
    "lodash": "4.17.21",
    "sass-loader": "^7.1.0",
    "tmp": "0.1.0",
    "vue": "2.6.10",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.35.2",
    "webpack-build-notifier": "1.0.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_print_server"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
